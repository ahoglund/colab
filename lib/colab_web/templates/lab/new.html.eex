<h1>New Lab</h1>
<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <%= form_for @conn, lab_path(@conn, :create), [as: :lab], fn f -> %>
      <div class="form-group">
        <%= text_input f, :name, placeholder: "Name", class: "form-control" %>
      </div>
      <%= submit "Create", class: "btn btn-primary" %>
    <% end %>
    </div>
  </div>
</div>
